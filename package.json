{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "lint": "run-s prettier eslint",
    "lint:fix": "run-s prettier:fix eslint:fix",
    "package:fix": "format-package -w",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "sasslint": "sass-lint -v",
    "sasslint:fix": "sass-lint-auto-fix",
    "start": "next start",
    "test": "mocha",
    "test:coverage": "nyc --reporter=text --reporter=html mocha",
    "test:watch": "npm run test -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,yaml}": "prettier --write"
  },
  "dependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "chai": "^4.3.4",
    "date-fns": "^2.23.0",
    "format-package": "^6.1.0",
    "gray-matter": "^4.0.3",
    "mocha": "^8.4.0",
    "next": "^10.2.3",
    "node-mocks-http": "^1.10.1",
    "npm-run-all": "^4.1.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "^6.0.3",
    "remark": "^13.0.0",
    "remark-html": "^13.0.2",
    "sass": "^1.39.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.17.15",
    "@types/react": "^17.0.20",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^5.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "^10.5.4",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "react-element-to-jsx-string": "^14.3.2",
    "sass-lint-auto-fix": "^0.21.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.3"
  }
}
